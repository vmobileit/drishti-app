        <!--<span lang="" class="question-label active">Asha name, </span>-->
        <!--<input autocomplete="off" type="text" name="/ANC_Registration_EngKan/asha_name" data-type-xml="string"/>-->
    <!--</label>-->
    <!--<label class="question non-select ">-->
        <!--<span lang="" class="question-label active">Asha phone number, -->
        <!--</span>-->
        <!--<input autocomplete="off" type="number" name="/ANC_Registration_EngKan/asha_phone_number"-->
               <!--data-constraint=".&gt; 999999999 and .&lt; 10000000000" data-type-xml="int"/>-->
