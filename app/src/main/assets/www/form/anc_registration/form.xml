    <label class="question non-select ">
        <span lang="" class="question-label active">ANC number </span>
        <span class="required">*</span>
        <input autocomplete="off" type="number" name="/ANC_Registration_EngKan/anc_number" required="required"
               data-constraint=".&gt; 0" data-type-xml="int"/>
    </label>
