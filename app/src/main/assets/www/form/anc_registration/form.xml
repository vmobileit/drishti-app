    <label class="question non-select ">
        <span lang="" class="question-label active">LMP
        </span>
        <span class="required">*</span>
        <input autocomplete="off" type="date" name="/ANC_Registration_EngKan/lmp_date" required="required"
               data-constraint=".&lt;= /ANC_Registration_EngKan/today " data-type-xml="date"/>
    </label>
    <label class="note non-select ">
        <span lang="" class="question-label active">EDD
            <span class="or-output" data-value=" /ANC_Registration_EngKan/edd_formatted ">
            </span>
        </span>
        <input autocomplete="off" type="text" name="/ANC_Registration_EngKan/edd_note" data-type-xml="string"
               readonly="readonly"/>
    </label>
    <fieldset class="question ">
        <fieldset>
            <legend>
                <span lang="" class="question-label active">Linked facility for delivery? 
                </span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input autocomplete="off" type="radio" name="/ANC_Registration_EngKan/linked_facility_delivery"
                           data-name="/ANC_Registration_EngKan/linked_facility_delivery" value="sub_center"
                           data-type-xml="select1"/>
                    <span lang="" class="option-label active">Sub Center </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="radio" name="/ANC_Registration_EngKan/linked_facility_delivery"
                           data-name="/ANC_Registration_EngKan/linked_facility_delivery" value="phc"
                           data-type-xml="select1"/>
                    <span lang="" class="option-label active">PHC </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="radio" name="/ANC_Registration_EngKan/linked_facility_delivery"
                           data-name="/ANC_Registration_EngKan/linked_facility_delivery" value="chc"
                           data-type-xml="select1"/>
                    <span lang="" class="option-label active">CHC </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="radio" name="/ANC_Registration_EngKan/linked_facility_delivery"
                           data-name="/ANC_Registration_EngKan/linked_facility_delivery" value="sdh"
                           data-type-xml="select1"/>
                    <span lang="" class="option-label active">SDH </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="radio" name="/ANC_Registration_EngKan/linked_facility_delivery"
                           data-name="/ANC_Registration_EngKan/linked_facility_delivery" value="dh"
                           data-type-xml="select1"/>
                    <span lang="" class="option-label active">DH </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="radio" name="/ANC_Registration_EngKan/linked_facility_delivery"
                           data-name="/ANC_Registration_EngKan/linked_facility_delivery" value="private"
                           data-type-xml="select1"/>
                    <span lang="" class="option-label active">Private </span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <label class="question or-branch pre-init non-select ">
        <span lang="" class="question-label active">Name of facility? 
        </span>
        <input autocomplete="off" type="text" name="/ANC_Registration_EngKan/name_facility"
               data-relevant=" /ANC_Registration_EngKan/linked_facility_delivery  != ''" data-type-xml="string"/>
    </label>
    <fieldset class="question or-branch pre-init ">
        <fieldset>
            <legend>
                <span lang="" class="question-label active">Had complications in any previous pregnancy? 
                </span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/complication"
                           value="Prev_Preg_Complication:_APH"
                           data-relevant=" /ANC_Registration_EngKan/case_num_pregnancies  &gt; 0"
                           data-type-xml="select"/>
                    <span lang="" class="option-label active">APH </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/complication"
                           value="Prev_Preg_Complication:_PIH"
                           data-relevant=" /ANC_Registration_EngKan/case_num_pregnancies  &gt; 0"
                           data-type-xml="select"/>
                    <span lang="" class="option-label active">PIH </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/complication"
                           value="Prev_Preg_Complication:_Preeclampsia_Eclampsia"
                           data-relevant=" /ANC_Registration_EngKan/case_num_pregnancies  &gt; 0"
                           data-type-xml="select"/>
                    <span lang="" class="option-label active">Pre-eclampsia / Eclampsia </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/complication"
                           value="Prev_Preg_Complication:_Pregnancy_with_Tumors"
                           data-relevant=" /ANC_Registration_EngKan/case_num_pregnancies  &gt; 0"
                           data-type-xml="select"/>
                    <span lang="" class="option-label active">Pregnancy with Tumors </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/complication"
                           value="Prev_Preg_Complication:_Prolonged_Obstructed_Labor"
                           data-relevant=" /ANC_Registration_EngKan/case_num_pregnancies  &gt; 0"
                           data-type-xml="select"/>
                    <span lang="" class="option-label active">Prolonged / Obstructed Labor </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/complication"
                           value="Prev_Preg_Complication:_Intrauterine_Growth_Retardation"
                           data-relevant=" /ANC_Registration_EngKan/case_num_pregnancies  &gt; 0"
                           data-type-xml="select"/>
                    <span lang="" class="option-label active">Intrauterine Growth Retardation </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/complication"
                           value="Prev_Preg_Complication:_PPH"
                           data-relevant=" /ANC_Registration_EngKan/case_num_pregnancies  &gt; 0"
                           data-type-xml="select"/>
                    <span lang="" class="option-label active">PPH </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/complication"
                           value="Prev_Preg_Complication:_Cesarean"
                           data-relevant=" /ANC_Registration_EngKan/case_num_pregnancies  &gt; 0"
                           data-type-xml="select"/>
                    <span lang="" class="option-label active">Cesarean </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/complication"
                           value="Prev_Preg_Complication:_Still_Birth"
                           data-relevant=" /ANC_Registration_EngKan/case_num_pregnancies  &gt; 0"
                           data-type-xml="select"/>
                    <span lang="" class="option-label active">Still Birth </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/complication"
                           value="Prev_Preg_Complication:_Spontaneous_Abortion"
                           data-relevant=" /ANC_Registration_EngKan/case_num_pregnancies  &gt; 0"
                           data-type-xml="select"/>
                    <span lang="" class="option-label active">Spontaneous Abortion </span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <fieldset class="question ">
        <fieldset>
            <legend>
                <span lang="" class="question-label active">Does woman have any of the following medical conditions?
                    
                </span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/medical_histories"
                           value="Medical_History:_TB" data-type-xml="select"/>
                    <span lang="" class="option-label active">TB </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/medical_histories"
                           value="Medical_History:_Hypertension" data-type-xml="select"/>
                    <span lang="" class="option-label active">Hypertension </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/medical_histories"
                           value="Medical_History:_Heart_Related_Diseases" data-type-xml="select"/>
                    <span lang="" class="option-label active">Heart-related diseases </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="checkbox" name="/ANC_Registration_EngKan/medical_histories"
                           value="Medical_History:_Diabetes" data-type-xml="select"/>
                    <span lang="" class="option-label active">Diabetes </span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <label class="question or-appearance-minimal ">
        <span lang="" class="question-label active">Woman's blood group? 
        </span>
        <select name="/ANC_Registration_EngKan/w_blood_group" data-name="/ANC_Registration_EngKan/w_blood_group"
                data-type-xml="select1">
            <option value="">...</option>
            <option value="a_positive">A+</option>
            <option value="a_negative">A-</option>
            <option value="b_positive">B+</option>
            <option value="b_negative">B-</option>
            <option value="ab_positive">AB+</option>
            <option value="ab_negative">AB-</option>
            <option value="o_positive">O+</option>
            <option value="o_negative">O-</option>
        </select>
        <span class="or-option-translations" style="display:none;">
        </span>
    </label>
    <label class="question non-select ">
        <span lang="" class="question-label active">Height (in cms) 
        </span>
        <input autocomplete="off" type="number" name="/ANC_Registration_EngKan/height" data-type-xml="decimal"
               step="any"/>
    </label>
    <label class="question non-select ">
        <span lang="" class="question-label active">Weight at start of pregnancy (in kgs) 
        </span>
        <input autocomplete="off" type="number" name="/ANC_Registration_EngKan/weight_startofpregnancy"
               data-type-xml="decimal" step="any"/>
    </label>
    <label class="note or-branch pre-init non-select ">
        <span lang="" class="question-label active">
            <span class="or-output" data-value=" /ANC_Registration_EngKan/woman_name ">
            </span>is at risk because of,
            <span class="or-output" data-value=" /ANC_Registration_EngKan/woman_name ">
            </span>
            
            <span class="or-output" data-value=" /ANC_Registration_EngKan/high_risk_reason ">
            </span>
        </span>
        <input autocomplete="off" type="text" name="/ANC_Registration_EngKan/high_risk_note"
               data-relevant=" /ANC_Registration_EngKan/is_high_risk1  = 'yes'" data-type-xml="string"
               readonly="readonly"/>
    </label>
    <fieldset class="question ">
        <fieldset>
            <legend>
                <span lang="" class="question-label active">Do you want to mark as HRP? 
                </span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input autocomplete="off" type="radio" name="/ANC_Registration_EngKan/is_high_risk"
                           data-name="/ANC_Registration_EngKan/is_high_risk" value="yes" data-type-xml="select1"/>
                    <span lang="" class="option-label active">Yes </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="radio" name="/ANC_Registration_EngKan/is_high_risk"
                           data-name="/ANC_Registration_EngKan/is_high_risk" value="no" data-type-xml="select1"/>
                    <span lang="" class="option-label active">No </span>
                </label>
            </div>
        </fieldset>
    </fieldset>

    <fieldset id="or-preload-items" style="display:none;">
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/ANC_Registration_EngKan/today" data-preload="date"
                   data-preload-params="today" data-type-xml="date"/>
        </label>
    </fieldset>
    <fieldset id="or-calculated-items" style="display:none;">
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/ANC_Registration_EngKan/woman_name"
                   data-calculate="concat( /ANC_Registration_EngKan/case_woman_name , ' ')" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/ANC_Registration_EngKan/num_pregnancies"
                   data-calculate=" /ANC_Registration_EngKan/case_num_pregnancies  + 1" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/ANC_Registration_EngKan/edd"
                   data-calculate="date( /ANC_Registration_EngKan/lmp_date +280)" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/ANC_Registration_EngKan/edd_formatted"
                   data-calculate="format-date( /ANC_Registration_EngKan/edd , '%Y-%n-%e')" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/ANC_Registration_EngKan/weight_risk"
                   data-calculate="if( /ANC_Registration_EngKan/weight_startofpregnancy  &lt; 45 or  /ANC_Registration_EngKan/weight_startofpregnancy  &gt;= 90, 'Weight', '')"
                   data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/ANC_Registration_EngKan/height_risk"
                   data-calculate="if( /ANC_Registration_EngKan/height  &lt;= 145, 'Height', '')"
                   data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/ANC_Registration_EngKan/is_high_risk_till_delivery"
                   data-calculate="if( /ANC_Registration_EngKan/case_would_be_high_risk  = 'yes' or  /ANC_Registration_EngKan/weight_risk  != '' or  /ANC_Registration_EngKan/height_risk  != '' or  /ANC_Registration_EngKan/complication  != '', 'yes', 'no')"
                   data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/ANC_Registration_EngKan/is_high_risk_till_delivery_reason"
                   data-calculate="concat( /ANC_Registration_EngKan/case_would_be_high_risk_reason , ' ',  /ANC_Registration_EngKan/weight_risk , ' ',  /ANC_Registration_EngKan/height_risk , ' ', string( /ANC_Registration_EngKan/complication ))"
                   data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/ANC_Registration_EngKan/is_high_risk_till_pnc_close"
                   data-calculate="if( /ANC_Registration_EngKan/medical_histories  != '', 'yes', 'no')"
                   data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/ANC_Registration_EngKan/is_high_risk_till_pnc_close_reason"
                   data-calculate="string( /ANC_Registration_EngKan/medical_histories )" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/ANC_Registration_EngKan/is_high_risk1"
                   data-calculate="if( /ANC_Registration_EngKan/is_high_risk_till_delivery  = 'yes' or  /ANC_Registration_EngKan/is_high_risk_till_pnc_close  = 'yes', 'yes', 'no')"
                   data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/ANC_Registration_EngKan/high_risk_reason"
                   data-calculate="concat( /ANC_Registration_EngKan/is_high_risk_till_delivery_reason , ' ',  /ANC_Registration_EngKan/is_high_risk_till_pnc_close_reason )"
                   data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/ANC_Registration_EngKan/meta/instanceID"
                   data-calculate="concat('uuid:', uuid())" data-type-xml="string"/>
        </label>
        <label class="calculation non-select ">
            <input autocomplete="off" type="hidden" name="/ANC_Registration_EngKan/formhub/uuid"
                   data-calculate="'698bfaa8efe244e591896db01ff5693b'" data-type-xml="string"/>
        </label>
    </fieldset>
</form>
