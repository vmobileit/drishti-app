<form autocomplete="off" novalidate="novalidate" class="or clearfix" id="ANC_Registration_EngKan">
    <!--This form was created by transforming a OpenRosa-flavored (X)Form using an XSLT sheet created by Enketo LLC.-->
    <section class="form-logo"></section>
    <h3 id="form-title">ANC Registration</h3>

    <label class="question non-select ">
        <span lang="" class="question-label active">EC Number: </span>
        <input autocomplete="off" type="text" name="/ANC_Registration_EngKan/ec_num" data-type-xml="int" disabled="disabled"/>
    </label>


    <label class="question non-select ">
        <span lang="" class="question-label active">Women Name: </span>
        <input autocomplete="off" type="text" name="/ANC_Registration_EngKan/woman_name" data-type-xml="string" disabled="disabled"/>
    </label>

    <label class="question non-select ">
        <span lang="" class="question-label active">Registration date:
        </span>
        <span class="required">*</span>
        <input autocomplete="off" type="date" name="/ANC_Registration_EngKan/reg_date" required="required"
               data-constraint=".&lt;= /ANC_Registration_EngKan/today " data-type-xml="date"/>
    </label>
    <fieldset class="question ">
        <fieldset>
            <legend>
                <span lang="" class="question-label active">Registration place: </span>
                <span class="required">*</span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input autocomplete="off" type="radio" name="/ANC_Registration_EngKan/reg_place"
                           data-name="/ANC_Registration_EngKan/reg_place" value="sub_center" required="required"
                           data-type-xml="select1"/>
                    <span lang="" class="option-label active">My Sub Center </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="radio" name="/ANC_Registration_EngKan/reg_place"
                           data-name="/ANC_Registration_EngKan/reg_place" value="phc" required="required"
                           data-type-xml="select1"/>
                    <span lang="" class="option-label active">My PHC </span>
                </label>
                <label class="">
                    <input autocomplete="off" type="radio" name="/ANC_Registration_EngKan/reg_place"
                           data-name="/ANC_Registration_EngKan/reg_place" value="elsewhere" required="required"
                           data-type-xml="select1"/>
                    <span lang="" class="option-label active">Elsewhere
                        </span>
                </label>
            </div>
        </fieldset>
    </fieldset>

